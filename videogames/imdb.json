{
  "recipe_shortcut": "imdb_videogames",
  "list_type": "videogames",
  "engine_version": 16,
  "title": "IMDB Videogames",
  "description": "A recipe to get a list of videogames",
  "urls": [
    "https://m.imdb.com/title/"
  ],
  "headers": {
    "Accept-Language": "en-UK,en",
    "User-Agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_15_2) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/81.0.4044.113 Safari/537.36"
  },
  "autocomplete_steps": [
    {
      "command": "load",
      "url": "https://www.imdb.com/find/?q=$INPUT&s=tt&ttype=vg",
      "config": {
        "js": true,
        "timeout": 1
      },
      "description": "Searches videogames by title"
    },
    {
      "command": "store_attribute",
      "locator": "ul.ipc-metadata-list > li:nth-child($i) img",
      "attribute_name": "src",
      "output": {
        "name": "COVER$i"
      },
      "config": {
        "loop": {
          "index": "i",
          "from": 1,
          "to": 6,
          "step": 1
        }
      }
    },
    {
      "command": "store_text",
      "locator": "ul.ipc-metadata-list > li:nth-child($i) .ipc-title-link-wrapper h3",
      "output": {
        "name": "TITLE$i"
      },
      "config": {
        "loop": {
          "index": "i",
          "from": 1,
          "to": 6,
          "step": 1
        }
      }
    },
    {
      "command": "store_text",
      "locator": "ul.ipc-metadata-list > li:nth-child($i) .cli-title-metadata",
      "output": {
        "name": "SUBTITLE$i"
      },
      "config": {
        "loop": {
          "index": "i",
          "from": 1,
          "to": 6,
          "step": 1
        }
      }
    },
    {
      "command": "regex",
      "input": "$SUBTITLE$i",
      "expression": "(\\d\\d\\d\\d)",
      "output": {
        "name": "SUBTITLE$i"
      },
      "config": {
        "loop": {
          "index": "i",
          "from": 1,
          "to": 6,
          "step": 1
        }
      }
    },
    {
      "command": "store_attribute",
      "locator": "ul.ipc-metadata-list > li:nth-child($i) .ipc-title-link-wrapper",
      "attribute_name": "href",
      "output": {
        "name": "URL$i"
      },
      "config": {
        "loop": {
          "index": "i",
          "from": 1,
          "to": 6,
          "step": 1
        }
      }
    },
    {
      "command": "store",
      "input": "https://m.imdb.com$URL$i",
      "output": {
        "name": "URL$i"
      },
      "config": {
        "loop": {
          "index": "i",
          "from": 1,
          "to": 6,
          "step": 1
        }
      }
    }
  ],
  "url_steps": [
    {
      "command": "load",
      "url": "$INPUT",
      "config": {
        "js": true,
        "timeout": 1
      },
      "description": "Load the url of the movie"
    },
    {
      "command": "store_url",
      "output": {
        "name": "URL"
      },
      "description": "Saves the URL of the detail"
    },
    {
      "command": "store_text",
      "locator": "h1",
      "output": {
        "name": "TITLE",
        "type": "string",
        "show": true
      },
      "description": "Takes the title directly from h1 element"
    },
    {
      "command": "store_attribute",
      "locator": "meta[property='og:title']",
      "attribute_name": "content",
      "output": {
        "name": "OG_TITLE"
      },
      "description": "Takes the og:title to extract the year"
    },
    {
      "command": "regex",
      "input": "$OG_TITLE",
      "expression": "([0-9]{4})",
      "output": {
        "name": "DATE",
        "type": "date",
        "format": "YYYY",
        "show": true
      },
      "description": "Extracts the year using simple regex without lookahead"
    },
    {
      "command": "store_attribute",
      "locator": "meta[name='description']",
      "attribute_name": "content",
      "output": {
        "name": "DESCRIPTION",
        "type": "string",
        "show": true
      },
      "description": "Saves the description from meta tag"
    },
    {
      "command": "store_text",
      "locator": "a[href*='ratings'] span span",
      "output": {
        "name": "RATING",
        "type": "string"
      },
      "description": "Saves the rating using href-based selector (works in WebView)"
    },
    {
      "command": "regex",
      "input": "$RATING",
      "expression": "([0-9]+[,\\.][0-9]+)",
      "output": {
        "name": "RATING",
        "type": "float",
        "show": true
      },
      "description": "Extracts numeric rating value"
    },
    {
      "command": "store_attribute",
      "locator": "meta[property='og:image']",
      "attribute_name": "content",
      "output": {
        "name": "COVER",
        "type": "string",
        "show": true
      },
      "description": "Saves the image directly from og:image meta tag"
    },
    {
      "command": "regex",
      "input": "$URL",
      "expression": "https://m\\.(.*)",
      "output": {
        "name": "KEYWORDS"
      },
      "description": "Extract domain and path after m."
    },
    {
      "command": "store",
      "input": "https://www.$KEYWORDSkeywords",
      "output": {
        "name": "KEYWORDS"
      },
      "description": "Build desktop URL for keywords page"
    },
    {
      "command": "load",
      "url": "$KEYWORDS",
      "description": "Load the url of the keywords"
    },
    {
      "command": "store_array",
      "locator": ".ipc-metadata-list-summary-item__t[href*='playstation']",
      "output": {
        "name": "TAGS",
        "type": "string",
        "show": true
      },
      "description": "List of platforms"
    },
